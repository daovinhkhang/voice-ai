# Vietnamese AI Voice Chat System - Environment Variables Template

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face Configuration (for TTS models)
HUGGINGFACE_TOKEN=your_huggingface_token_here

# Audio Settings
SAMPLE_RATE=16000
CHUNK_SIZE=1024
CHANNELS=1

# Model Settings
STT_MODEL=whisper-1
LLM_MODEL=gpt-3.5-turbo
TTS_MODEL=zalopay/vietnamese-tts

# Performance Settings
MAX_AUDIO_LENGTH=30
RESPONSE_TIMEOUT=10

# Web Server Settings
PORT=8080
DEBUG=False
